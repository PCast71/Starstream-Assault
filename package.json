  "license": "ISC",
  "dependencies": {
    "http-proxy-middleware": "^3.0.0",
    "phaser": "^3.55.2",
    "react-phaser-fiber": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.1.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
